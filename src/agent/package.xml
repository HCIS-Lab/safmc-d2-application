<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>agent</name>
  <version>0.0.0</version>
  <description>ROS 2 package for controlling the drone in the SAFMC D2 competition. Runs on the
    drone's Raspberry Pi, handling flight control and mission execution.</description>
  <maintainer email="efliao@cs.nycu.edu.tw">root</maintainer>
  <license>Internal Use Only</license>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>